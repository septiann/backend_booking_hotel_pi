<!-- DataTales Example -->
<div class="tab-pane fade show active" id="show-image" role="tabpanel" aria-labelledby="home-tab">
    <div class="row">
        <% for(let x = 0; x < item.image_id.length; x++) { %>
        <div class="col-md-4">
            <div class="card shadow mb-4 mt-2">
              <div class="card-body">
                    <img src="https://backend-inncation.herokuapp.com/<%= item.image_id[x].image_url %>" alt="<%= item.image_id[x]._id %>" style="width: 100%; display: block;">
                </div>
            </div>
        </div>
        <% } %>
    </div>
    <a href="/admin/item" class="btn btn-secondary">Kembali</a>
</div>